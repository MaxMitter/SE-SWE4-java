<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="138.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="swe4.gui.controller.GameListEntryController">
   <VBox prefHeight="144.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
      <children>
         <HBox prefHeight="47.0">
            <children>
               <Label fx:id="lbl_gameTime" prefHeight="33.0" text="15:00">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </HBox.margin>
               </Label>
               <Circle fx:id="crcl_live" fill="#eb1d1d" radius="7.0" stroke="BLACK" strokeType="INSIDE" visible="false">
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="15.0" />
                  </HBox.margin>
               </Circle>
               <Label fx:id="lbl_gameName" prefHeight="33.0" text="Game 1">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </HBox.margin>
               </Label>
               <Label fx:id="lbl_userPoints" text="Punkte: " HBox.hgrow="ALWAYS">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </HBox.margin>
               </Label>
            </children>
         </HBox>
         <HBox prefHeight="100.0" VBox.vgrow="ALWAYS">
            <children>
               <Label fx:id="lbl_team1" onMouseClicked="#lblTeam1Clicked" prefHeight="33.0" text="Team 1">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <padding>
                     <Insets left="10.0" right="10.0" />
                  </padding>
               </Label>
               <Region prefHeight="200.0" HBox.hgrow="ALWAYS" />
               <Label fx:id="lbl_team1Score" alignment="CENTER" contentDisplay="RIGHT" nodeOrientation="RIGHT_TO_LEFT" prefHeight="33.0" text="0" HBox.hgrow="ALWAYS">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <HBox.margin>
                     <Insets left="10.0" right="10.0" />
                  </HBox.margin>
               </Label>
            </children>
         </HBox>
         <HBox prefHeight="100.0" VBox.vgrow="ALWAYS">
            <children>
               <Label fx:id="lbl_team2" onMouseClicked="#lblTeam2Clicked" prefHeight="33.0" text="Team 2">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <padding>
                     <Insets left="10.0" right="10.0" />
                  </padding>
               </Label>
               <Region prefHeight="200.0" HBox.hgrow="ALWAYS" />
               <Label fx:id="lbl_team2Score" alignment="CENTER" prefHeight="33.0" text="0">
                  <font>
                     <Font size="18.0" />
                  </font>
                  <HBox.margin>
                     <Insets left="10.0" right="10.0" />
                  </HBox.margin>
               </Label>
            </children>
         </HBox>
      </children>
   </VBox>
   <Label fx:id="lbl_gameId" visible="false" />
</AnchorPane>
